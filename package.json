{
  "name": "sbremit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -f .env.development react-scripts start",
    "build": "env-cmd -f .env.staging react-scripts build",
    "build:staging": "CI=false env-cmd -f .env.staging react-scripts build",
    "build:production": "CI=false env-cmd -f .env.production react-scripts build",
    "test": "concurrently \"PORT=3008 npm start\" \"cypress open --browser chrome\"",
    "eject": "react-scripts eject",
    "e2e-open": "concurrently \"npm start\" \"cypress open\"",
    "e2e": "concurrently \"npm start\" \"cypress run\""
  },
